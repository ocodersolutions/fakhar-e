                if($('.mainmenu').hasClass('showmenu1'))
                    $('.mainmenu').removeClass('showmenu1').addClass('hidemenu1');
            }
            else
            {
                if($('.mainmenu').hasClass('hidemenu'))
                    $('.mainmenu').removeClass('hidemenu').addClass('showmenu');

                if($('.mainmenu').hasClass('hidemenu1'))
                    $('.mainmenu').removeClass('hidemenu1').addClass('showmenu1');
