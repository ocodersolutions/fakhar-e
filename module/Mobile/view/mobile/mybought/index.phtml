<script type="text/javascript">

    $(function () {

        // init the tree
        $('#tree').aciTree({
            ajax: {
                url: '/attributes/get-categories-json?mapped=true&ec=<?= $this->excludeCats ?>&levels=3&id='
            },
            checkbox: true
        });

        $('#tree').on('acitree', function (event, api, item, eventName, options) {
            switch (eventName) {
                case 'checked':
                    //alert(item);
                    //alert('checked: ' + api.getId(item));
                    //break;
                case 'unchecked':
                    getAllCheckedElements(event, api, item, eventName, options);
                    loadProductData();
                    break;
                case 'init' :
                    getAllCheckedElements(event, api, item, eventName, options);
                    break;
            }
        });

    });

    function getAllCheckedElements(event, api, item, eventName, options)
    {
        var checkedElements = $("#tree .aciTreeLi .aciTreeLeaf");
        checkedElementsArr = api.checkboxes(checkedElements, true);
        var catIdStr = '';
        checkedElementsArr.each(function (index, item) {
            var $item = $(item);
            catIdStr += api.getId($item) + ',';
            //console.log(api.getId($item) + ": " + api.getLabel($item));
        });
        $("#catids").val(catIdStr);
    }

</script>

