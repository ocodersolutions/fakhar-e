    var loading_outfit = 0;
    var endData_outfit = 0;
    function loadOutfitData(isLazyLoading)
    {
        loading_outfit = 1;
        var url = "/mybought/getoutfits";
        var filterData = $("#outfitFilterDetail").serialize();
        $.ajax({
            url: url,
            type: 'POST',
            data:  filterData,
            dataType: 'html',
            beforeSend:function(){
                showAjxLoadingDiv();
            },
            success: function (result) {
                hideAjxLoadingDiv();
                //var json = jQuery.parseJSON(result);
                //console.log(result);
                loading = 0;
                if (result.indexOf('No Record') > 0)
                {
                    endData_outfit = 1;
                }
                else
                {
                    $("#outfit_page").val(parseInt($("#outfit_page").val()) + 1);
                    $("#outfit-page .grid").append(result);
                    var highestBox = 0;
                    var highestBox1 = 0;
                    $('.grid-row.resize').each(function(index,value){  
                        if(index == 0) {
                            highestBox = $(this).find(".bigger:first").height();
                            highestBox1 = $(this).find(".smaller:first").height();
                        }
                  
                        $(this).find(".resize_follow_big").each(function(){
                            $(this).height(highestBox);
                        });
                   
                        $(this).find(".resize_follow_small").each(function(){
                            $(this).height(highestBox1);
                        });
                    });
                }

        }});
    }

